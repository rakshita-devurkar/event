<h1> All events </h1> <br>
<table border =1>
  <tr>
    <td> Eventname </td>
    <td> Location </td>
    <td> Date </td>
  </tr>
  <% for(var i=0;i<inserted.length;i++){%>
  <tr>
    <td>
      <a href = "/event/findone?id= <%= inserted[i].id%>">
      <%= inserted[i].name %> </a>
    </td>
    <td>
      <%= inserted[i].location %>
    </td>
    <td>
      <%= inserted[i].date %>
    </td>
  </tr>
  <% } %>
</table> <br><br><br>
<a href = "/event/homepage"> Back to Homepage </a>